    "# Climbing Stairs\n",
    "\n",
    "DP problem\n",
    "\n",
    "draw a tree to denote it "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def climbing(n):\n",
    "    one  =  1\n",
    "    two = 1 \n",
    "\n",
    "    for n in range(n-1):\n",
    "        temp = one\n",
    "        one = one + two\n",
    "        two = temp\n",
    "    return one\n",
    "\n",
    "\n",
    "\n",
    "\n"
   ]
