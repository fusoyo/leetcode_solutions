    "class Solution:\n",
    "\n",
    "    def palindrome_num(self, x):\n",
    "        if x < 0:\n",
    "            return False\n",
    "        reversed_x = self.reversed_int(x)\n",
    "\n",
    "        if reversed_x == x:\n",
    "            return True\n",
    "        else:\n",
    "            return False"
