   "source": [
    "def merge_sort(list):\n",
    "    \"\"\"sorts a list in ascending order, returns a new sorted list.\n",
    "        Divide: \n",
    "        Conquer:\n",
    "        Combine:\n",
    "    \"\"\"\n",
    "    if len(list) <= 1:\n",
    "        return list\n",
    "\n",
    "    left_half, right_half = split(list)\n",
    "\n",
    "    left = merge_sort(left_half)\n",
    "    right = merge_sort(right_half)\n",
    "\n",
    "    return merge(left, right)\n",
    "\n",
    "\n",
    "def merge(left, right):\n",
    "    l = []\n",
    "    i = 0\n",
    "    j = 0\n",
    "    while i < len(left) and j < len(right):\n",
    "        if left[i] <= right[j]:\n",
    "            l.append(left[i])\n",
    "            i += 1\n",
    "        else:\n",
    "            l.append(right[j])\n",
    "            j += 1\n",
    "    while i < len(left):\n",
    "        l.append(left[i])\n",
    "        i += 1\n",
    "    while j < len(right):\n",
    "        l.append(right[j])\n",
    "        j += 1\n",
    "\n",
    "    return l\n",
    "\n",
    "\n",
    "def split(list):\n",
    "    \"\"\"sublists of lists\"\"\"\n",
    "    mid = len(list) // 2\n",
    "    left = list[:mid]\n",
    "    right = list[mid:]\n",
    "\n",
    "    return left, right\n",
    "\n",
    "\n",
    "\n"
   ]
